import { GeoPoint, Timestamp, DocumentReference } from "firebase/firestore";

/**
 * Export interface of <%- name %> collection.
 */
export interface <%- name %>Interface {
<% columns.forEach((column) => { -%>
    <%- column.c_name %>: <%- function(type) {
        if(["array", "map"].includes(type)){
            return column.memo;
        } else {
            return dataType.find(x => x.FirestoreType === type).DBType;
        }
    }(column.c_datatype) %>;
<% }); -%>
};